    "url_stack": {
        "prefix": "url_stack",
        "body": [
            "${1|url,re_path|}(",
            "\tr'^${2:REGEX}/$',",
            "\t${3:VIEW}${4:.as_view()},",
            "\tname='$5'",
            "),"
